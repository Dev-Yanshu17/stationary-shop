<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9EDF6"
    tools:context=".more_details_profile">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/l11">
        <TextView
            android:id="@+id/tv8"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/baseline_arrow_back_brown"
            android:onClick="call_backsignup"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="info"
            android:textColor="#0D526A"
            android:textSize="28dp"
            android:textStyle="bold"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="12dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/l11"
        android:layout_marginTop="30dp"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l1"
            android:onClick="home"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="50dp"
                android:layout_height="50dp">
                <ImageView
                    android:layout_width="33dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginLeft="13dp"
                    android:src="@drawable/hom06" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Home"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>

            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:layout_margin="5dp"
                android:src="@drawable/right_arow" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l2"
            android:onClick="order"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/order6"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Order"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l3"
            android:onClick="wishlist"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/wishlist6"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Wishlist"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l4"
            android:onClick="shareapp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="50dp"
                android:layout_height="50dp">
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="40dp"
                    android:layout_marginTop="8dp"
                    android:layout_marginLeft="13dp"
                    android:src="@drawable/share06" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Share App"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l6"
            android:onClick="changepassword"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@drawable/changepassword"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Change Password"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l5"
            android:onClick="profile"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@drawable/profile06"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Profile"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l8"
            android:onClick="aboutus"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@drawable/aboutus6"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="About us"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>

            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/right_arow" />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l0"
            android:onClick="rating"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@drawable/rate"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Rating"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="55dp"
            android:layout_marginTop="10dp"
            android:id="@+id/l7"
            android:onClick="logout"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="40dp"
                android:layout_height="50dp"
                android:src="@drawable/logout"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="310dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp">
                <TextView
                    android:layout_width="300dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="10dp"
                    android:text="Logout"
                    android:gravity="center_vertical"
                    android:textStyle="bold"
                    android:textSize="20dp"/>
            </LinearLayout>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="50dp"
                android:src="@drawable/right_arow"
                android:layout_margin="5dp"/>
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1.3dp"
            android:background="#136988"/>
    </LinearLayout>
</RelativeLayout>